-- Shift + Q Hides Gui

local Enabled = true
UserInputService.InputBegan:Connect(function(Input,Typing)
	if Input.KeyCode == Enum.KeyCode.RightControl then
		Enabled = not Enabled
		
		if Enabled == false then
			euronet:Hide()
		elseif Enabled == true then
			euronet:Show()
		end
	end
end)
